{
  "name": "pinyin-test",
  "version": "0.1.0",
  "private": true,
  "main": "./public/electron.js",
  "homepage": "./",
  "build": {
    "mac": {
      "category": "demo"
    },
    "files": [
      {
        "from": "./",
        "to": "./",
        "filter": [
          "**/*",
          "!node_module"
        ]
      }
    ],
    "extraFiles": [
      "dll",
      "db"
    ]
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "electron-dev": "set NODE_ENV=development&&electron .",
    "electron-pack": "yarn build && electron-builder build --ia32"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "ffi-napi": "^2.4.5",
    "sqlite3": "^4.1.1"
  },
  "devDependencies": {
    "electron": "^6.0.10",
    "electron-builder": "^21.2.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-scripts": "3.2.0"
  }
}
